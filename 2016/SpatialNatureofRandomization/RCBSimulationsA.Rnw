rcb.efficiency.analysis <- function(current.dat) {
  sub1.dat <- subset(current.dat,current.dat$rep!=1)
  res1 <- rcb.analysis(sub1.dat,REML=FALSE)
  res1$MissingRep <- "1"
  res1$MissingRepCount <- 1
  
  sub2.dat <- subset(current.dat,current.dat$rep!=4)
  res2 <- rcb.analysis(sub2.dat,REML=FALSE)
  res2$MissingRep <- "4"
  res2$MissingRepCount <- 1
  
  sub3.dat <- subset(sub1.dat,sub1.dat$rep!=4)
  res3 <- rcb.analysis(sub3.dat,REML=FALSE)
  res3$MissingRep <- "1-4"
  res3$MissingRepCount <- 2
  
  sub4.dat <- subset(sub1.dat,sub1.dat$rep!=2)
  res4 <- rcb.analysis(sub4.dat,REML=FALSE)
  res4$MissingRep <- "1-2"
  res4$MissingRepCount <- 2
  
  sub5.dat <- subset(sub2.dat,sub2.dat$rep!=3)
  res5 <- rcb.analysis(sub5.dat,REML=FALSE)
  res5$MissingRep <- "3-4"
  res5$MissingRepCount <- 2
  return(rbind(res1,res2,res3,res4,res5))
}
